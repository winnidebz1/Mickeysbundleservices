# Moolre Payment Gateway Configuration
# Get your PRIVATE API Key from your Moolre Dashboard (do NOT use the Public Key)
MOOLRE_API_KEY=your_public_api_key_here
MOOLRE_API_USER=your_moolre_username
MOOLRE_ACCOUNT_NUMBER=your_moolre_wallet_account_number

# Webhook URL (will be automatically set by Vercel)
ADMIN_WEBHOOK_URL=https://your-domain.vercel.app/api/webhook

# Bundle Delivery API (if you have a separate provider)
BUNDLE_API_KEY=your_bundle_provider_api_key_here
BUNDLE_API_URL=https://api.your-bundle-provider.com/deliver
